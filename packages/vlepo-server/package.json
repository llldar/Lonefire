{
  "name": "@vlepo/server",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn prisma migrate deploy && yarn -s clean && tsc --outDir ./build",
    "clean": "rm -rf ./build",
    "db:clean": "cross-env DEBUG=vlepo:* ts-node prisma/seed.ts -c",
    "db:migrate": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push --preview-feature",
    "db:reset": "prisma migrate reset",
    "db:seed": "cross-env DEBUG=vlepo:* ts-node prisma/seed.ts -s",
    "dev": "cross-env NODE_ENV=development DEBUG=vlepo:* ts-node-dev -r dotenv/config --exit-child --tree-kill --respawn --transpile-only src/app.ts",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/graphql",
    "generate:prisma": "prisma generate",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "1.1.1",
    "@koa/cors": "3.1.0",
    "@prisma/client": "2.20.1",
    "@vlepo/shared": "1.0.0",
    "argon2": "0.27.2",
    "base-64": "1.0.0",
    "crypto-random-string": "3.3.1",
    "date-fns": "2.20.1",
    "debug": "4.3.1",
    "grant": "5.4.12",
    "graphql": "15.5.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-playground-middleware-koa": "1.6.21",
    "graphql-scalars": "1.9.0",
    "graphql-upload": "11.0.0",
    "image-size": "0.9.7",
    "jwt-decode": "3.1.2",
    "knex": "0.95.4",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-graphql": "0.8.0",
    "koa-graphql-batch": "1.1.0",
    "koa-mount": "4.0.0",
    "koa-router": "10.0.0",
    "koa-send": "5.0.1",
    "koa-session": "6.2.0",
    "koa-static": "5.0.0",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.33.0",
    "node-fetch": "2.6.1",
    "node-vibrant": "3.2.1-alpha.1",
    "oauth2-server": "3.1.1",
    "pg": "8.5.1",
    "reading-time": "1.3.0",
    "ts-pattern": "3.1.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/base-64": "0.1.3",
    "@types/debug": "4.1.5",
    "@types/faker": "5.5.1",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/graphql-upload": "8.0.4",
    "@types/koa": "2.13.1",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-graphql": "0.8.4",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.1",
    "@types/koa-send": "4.1.2",
    "@types/koa-session": "5.10.3",
    "@types/koa-static": "4.0.1",
    "@types/koa__cors": "3.0.2",
    "@types/node": "14.14.37",
    "@types/node-fetch": "2.5.10",
    "@types/oauth2-server": "3.0.12",
    "@types/uuid": "8.3.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.5.3",
    "meow": "9.0.0",
    "prisma": "2.20.1",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.4"
  }
}