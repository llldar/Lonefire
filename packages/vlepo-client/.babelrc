{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        },
        "preset-env": {
          "targets": {
            "browsers": [">1%", "not dead", "not ie <= 11", "not op_mini all"]
          }
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    ["relay", { "artifactDirectory": "./src/__generated__" }]
  ]
}
