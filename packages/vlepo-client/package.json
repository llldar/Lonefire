{
  "name": "@vlepo/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "yarn generate",
    "build": "next build",
    "debug": "NODE_OPTIONS='--inspect=5009' next dev -p ${PORT-3000}",
