{
  "name": "@vlepo/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "yarn generate",
    "build": "next build",
    "debug": "NODE_OPTIONS='--inspect=5009' next dev",
    "dev": "cross-env DEBUG=vlepo:* next dev",
    "generate": "yarn relay",
    "get-schema": "graphql-codegen -r dotenv/config --config .graphqlrc.yml",
    "lint": "eslint src --ext .ts,.tsx",
    "relay": "relay-compiler --src ./src --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' --exclude '**/schema/**' --schema ./src/schema/schema.graphql --language typescript --customScalars.DateTime=String --artifactDirectory ./src/__generated__",
    "relay:watch": "yarn relay --watch",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "watch": "yarn relay:watch"
  },
  "dependencies": {
    "@emotion-icons/material-outlined": "3.8.0",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@vlepo/shared": "1.0.0",
    "base-64": "1.0.0",
    "date-fns": "2.19.0",
    "debug": "4.3.1",
    "dotenv": "8.2.0",
    "graphql": "15.5.0",
    "masonic": "3.4.1",
    "next": "10.1.3",
    "next-mdx-remote": "2.1.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "7.0.0",
    "react-parallax-tilt": "1.4.96",
    "react-relay": "11.0.1",
    "react-relay-network-modern": "6.0.0",
    "react-relay-network-modern-ssr": "1.4.0",
    "react-share": "4.4.0",
    "react-spring": "9.0.0",
    "react-toast-notifications": "2.4.3",
    "react-typist": "2.0.5",
    "react-use-hoverintent": "1.2.3",
    "reading-time": "1.3.0",
    "recoil": "0.2.0",
    "relay-hooks": "4.2.0",
    "relay-runtime": "11.0.1",
    "remark-emoji": "2.1.0",
    "remark-images": "2.0.0",
    "styled-modal": "7.1.4",
    "styled-system": "5.1.5",
    "ts-pattern": "3.1.1"
  },
  "devDependencies": {
    "@graphql-cli/codegen": "1.17.22",
    "@graphql-codegen/schema-ast": "1.18.1",
    "@types/base-64": "0.1.3",
    "@types/debug": "4.1.5",
    "@types/eslint": "7.2.8",
    "@types/mdx-js__react": "1.5.3",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-relay": "11.0.1",
    "@types/react-typist": "2.0.1",
    "@types/relay-runtime": "10.1.10",
    "@types/styled-system": "5.1.10",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "babel-plugin-relay": "11.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.23.0",
    "graphql": "15.5.0",
    "graphql-cli": "4.1.0",
    "relay-compiler": "11.0.1",
    "relay-compiler-language-typescript": "13.0.9",
    "relay-config": "11.0.1",
    "typescript": "4.2.3",
    "typescript-styled-plugin": "0.15.0",
    "vscode-apollo-relay": "1.5.1"
  }
}