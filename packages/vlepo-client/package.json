{
  "name": "@vlepo/client",
  "version": "1.0.7",
  "private": true,
  "scripts": {
    "prebuild": "yarn generate",
    "build": "next build",
    "build:vercel": "yarn get-schema && yarn generate && yarn build",
    "debug": "NODE_OPTIONS='--inspect=5009' next dev",
    "dev": "DEBUG=vlepo* next dev",
