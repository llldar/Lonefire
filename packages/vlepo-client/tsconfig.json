{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ],
    "jsxImportSource": "@emotion/react",
    "jsx": "preserve",
    "baseUrl": ".",
    "plugins": [
      {
        "name": "typescript-styled-plugin"
      }
    ],
    "resolveJsonModule": true
  },
  "typeRoots": [
    "../node_modules/@types",
    "src/types"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
